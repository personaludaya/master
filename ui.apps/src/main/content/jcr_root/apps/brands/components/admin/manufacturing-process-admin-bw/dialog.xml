<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <admin
                jcr:primaryType="cq:Panel"
                title="Admin">
                <items jcr:primaryType="cq:WidgetCollection">
                    <process-name_t
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="The manufacturing process name."
                        fieldLabel="Process Name"
                        name="./process-name_t"
                        xtype="textfield"/>
                    <process-copy_t
                        jcr:primaryType="cq:Widget"
                        fieldDescription="The manufacturing process copy."
                        fieldLabel="Process Copy"
                        name="./process-copy_t"
                        xtype="textarea"/>
                    <process-img-path
                        jcr:primaryType="cq:Widget"
                        fieldDescription="The manufacturing process image path."
                        fieldLabel="Process Image Path"
                        name="./process-img-path"
                        xtype="pathfield"/>
                    <process-img-alt_t
                        jcr:primaryType="cq:Widget"
                        fieldDescription="The manufacturing process image alternate text. If this is blank, will use the “Process Name”."
                        fieldLabel="Process Image Alt Text"
                        name="./process-img-alt_t"
                        xtype="textfield"/>
                </items>
            </admin>
        </items>
    </items>
</jcr:root>
