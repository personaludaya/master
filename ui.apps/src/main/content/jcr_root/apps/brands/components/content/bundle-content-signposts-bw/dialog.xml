<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <bundle-signposts
                jcr:primaryType="cq:Panel"
                title="Bundle Signposts">
                <items jcr:primaryType="cq:WidgetCollection">
                     <bundle-signpost-paths
                        jcr:primaryType="cq:Widget"
                        fieldDescription="The bundle signpost paths."
                        fieldLabel="Bundle Signpost Paths"
                        name="./bundle-signpost-paths"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="pathfield"/>
                     </bundle-signpost-paths>
                </items>
            </bundle-signposts>
            <content-signpost
                jcr:primaryType="cq:Panel"
                title="Content Signpost">
                <items jcr:primaryType="cq:WidgetCollection">
                     <content-signpost-path
                        jcr:primaryType="cq:Widget"
                        fieldDescription="The content signpost admin path."
                        fieldLabel="Content Signpost Admin Path"
                        name="./content-signpost-path"
                        xtype="pathfield" />
                </items>
            </content-signpost>
        </items>
    </items>
</jcr:root>
