<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Search Result">
                <items jcr:primaryType="cq:WidgetCollection">
                    <search-default-img-path
                    	jcr:primaryType="cq:Widget"
                    	fieldDescription="This property is used to set the search default image placeholder in the search result when image is not found."
                    	fieldLabel="Search Default Image Path"
                    	name="./search-default-img-path"
                    	xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
