<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <general
                jcr:primaryType="cq:Panel"
                title="General">
                <items jcr:primaryType="cq:WidgetCollection">
                    <content_copy_line1_t
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Copy Line 1"
                        fieldDescription="The content copy line 1."
                        name="./content_copy_line1_t"
                        xtype="textfield"/>
                     <content_copy_line2_t
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Copy Line 2"
                        fieldDescription="The content copy line 2."
                        name="./content_copy_line2_t"
                        xtype="textfield"/>
                </items>
            </general>
        </items>
    </items>
</jcr:root>
