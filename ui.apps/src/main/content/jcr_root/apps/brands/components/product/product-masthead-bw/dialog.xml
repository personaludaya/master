<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <general
                jcr:primaryType="cq:Panel"
                title="General">
                <items jcr:primaryType="cq:WidgetCollection">
                    <prdt-tagline_t
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Product Tagline"
                        fieldDescription="The product tagline."
                        name="./prdt-tagline_t"
                        xtype="textfield"/>
                   <prdt-benefits_t
                        jcr:primaryType="cq:Widget"
                        fieldDescription="The product benefits."
                        fieldLabel="Product Benefits"
                        name="./prdt-benefits_t"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="textfield"/>
                    </prdt-benefits_t>
                </items>
            </general>
        </items>
    </items>
</jcr:root>
